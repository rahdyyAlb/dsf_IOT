<form method="post" action="{{ path('app_caisse_delete', {'id': caisse.id}) }}" onsubmit="return confirm('Voulez vous supprimer cette caisse?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ caisse.id) }}">
    <button class="btn btn-danger btn-small mt-2">Supprimer</button>
</form>
